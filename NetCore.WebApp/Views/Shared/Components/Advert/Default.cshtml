@using NetCore.ViewModels
@model List<ArticleModel>
@if (Model != null && Model.Count > 0)
{
    <div class="box_slide">
        
        @for (int i = 0; i < Model.Count; i++)
        {

            <div class="item">
                <a href="#">
                    <img src="~/images/srv_thumb.jpg" style="width: 100%"/>
                </a>
            </div>

        }
    </div>
}